{
    "builds": [{
            "src": "wind/wind/settings.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb","runtime": "python3.11.3"
            }
     }],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "wind/wind/staticfiles/$1"
        }
        ]
    }